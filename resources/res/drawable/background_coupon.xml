<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="3dp"/>
            <gradient android:startColor="#f6fbfd" android:endColor="#f6fbfd" android:angle="270"/>
            <stroke android:width="1dp" android:color="#b2c0cc" android:dashWidth="10px" android:dashGap="10px"/>
        </shape>
    </item>
</selector>
