<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <gradient android:startColor="#d6d6d6" android:endColor="#c9c7c7" android:angle="270"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <gradient android:startColor="#c9c7c7" android:endColor="#c9c7c7" android:angle="270"/>
        </shape>
    </item>
</selector>
