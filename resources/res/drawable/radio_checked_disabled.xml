<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="oval">
            <stroke android:width="2dp" android:color="@color/app_accent_color"/>
            <size android:height="20dp" android:width="20dp"/>
        </shape>
    </item>
    <item android:left="5dp" android:top="5dp" android:right="5dp" android:bottom="5dp">
        <shape android:shape="oval">
            <solid android:width="1dp" android:color="@color/app_accent_color"/>
            <size android:height="5dp" android:width="5dp"/>
        </shape>
    </item>
</layer-list>
