                    <Button android:id="@+id/com_appboy_inappmessage_full_button_two" style="@style/Appboy.InAppMessage.Button.Full.Two"/>
                </LinearLayout>
            </RelativeLayout>
        </LinearLayout>
        <RelativeLayout android:padding="10dp" android:layout_width="match_parent" android:layout_height="match_parent">
            <ImageButton android:id="@+id/com_appboy_inappmessage_full_close_button" style="@style/Appboy.InAppMessage.CloseButton.Full"/>
        </RelativeLayout>
    </RelativeLayout>
</com.appboy.ui.inappmessage.views.AppboyInAppMessageFullView>
