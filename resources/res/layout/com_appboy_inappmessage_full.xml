<?xml version="1.0" encoding="utf-8"?>
<com.appboy.ui.inappmessage.views.AppboyInAppMessageFullView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:padding="0dp" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:id="@+id/com_appboy_inappmessage_full_frame" style="@style/Appboy.InAppMessage.Frame.Full"/>
    <RelativeLayout android:id="@+id/com_appboy_inappmessage_full" style="@style/Appboy.InAppMessage.Full">
        <LinearLayout android:orientation="vertical" android:padding="0dp" android:layout_width="match_parent" android:layout_height="match_parent" android:weightSum="1">
            <ViewStub android:id="@+id/com_appboy_inappmessage_full_imageview_stub" android:layout="@layout/com_appboy_stubbed_inappmessage_image_view" style="@style/Appboy.InAppMessage.Image.Full"/>
            <RelativeLayout android:id="@+id/com_appboy_inappmessage_full_textarea" style="@style/Appboy.InAppMessage.Full.TextArea">
                <ScrollView android:id="@+id/com_appboy_inappmessage_full_scrollview" style="@style/Appboy.InAppMessage.ScrollView.Full">
                    <LinearLayout android:id="@+id/com_appboy_inappmessage_full_text_layout" style="@style/Appboy.InAppMessage.Layout.Text.Full">
                        <TextView android:id="@+id/com_appboy_inappmessage_full_header_text" style="@style/Appboy.InAppMessage.Header.Full"/>
